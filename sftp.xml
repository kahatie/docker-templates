<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
  <Category>tools</Category>
  <Name>SFTP</Name>
  <Description>
	Simple SFTP server
  </Description>
  <Overview>
    Simple SFTP server 
  </Overview>
  <GitHub>https://github.com/kahatie/docker-sftp</GitHub>
  <Repository>kahatie/sftp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>USER</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>PASSWORD</Name>
      <Value></Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/mnt/user</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Banner>https://raw.githubusercontent.com/kahatie/docker-templates/master/kahatie/dockerui.png</Banner>
  <Icon>https://raw.githubusercontent.com/kahatie/docker-templates/master/kahatie/dockerui.png</Icon>
</Containers>
