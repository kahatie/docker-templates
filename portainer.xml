<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>False</Beta>
  <Category>tools</Category>
  <Name>Portainer</Name>
  <Description>
  </Description>
  <Overview>
    Manage docker with GUI web
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/</Registry>
  <GitHub>https://github.com/portainer/portainer</GitHub>
  <Repository>portainer/portainer</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9000</HostPort>
        <ContainerPort>9000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:9000]/</WebUI>
  <Banner>https://raw.githubusercontent.com/kahatie/docker-templates/master/kahatie/portainer.png</Banner>
  <Icon>https://raw.githubusercontent.com/kahatie/docker-templates/master/kahatie/portainer.png</Icon>
</Containers>
